<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
			 xmlns:b.html='urn:import:org.gwtbootstrap3.client.ui.html'
			 xmlns:p='urn:import:cz.metacentrum.perun.wui.widgets'>

	<ui:with field="res" type="cz.metacentrum.perun.wui.profile.client.resources.PerunProfileResources"/>
	<ui:with field="msgRes" type="cz.metacentrum.perun.wui.profile.client.resources.PerunProfileTranslation"/>

	<b.html:Div>

		<b:Heading size="H2" ui:field="pageTitle">
			<b:Icon ui:field="icon" type="BUILDING_O" fixedWidth="true"/>
			<b.html:Text text="" ui:field="text"/>
			<b.html:Small text="{msgRes.menuGroups}" ui:field="small"/>
		</b:Heading>


		<b:Alert text="You are member of following virtual organizations. Click to name to see details."
				 ui:field="paragraph" visible="false" type="INFO" dismissable="true"/>
		<p:PerunLoader ui:field="loader"/>

		<b.html:Div ui:field="page">
		</b.html:Div>
	</b.html:Div>

</ui:UiBinder>